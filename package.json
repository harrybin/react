{
    "name": "react",
    "version": "1.0.0",
    "description": "React template",
    "author": "Harald Binkle",
    "license": "MIT",
    "type": "module",
    "engines": {
        "node": ">=20.10.0"
    },
    "browserslist": [
        "defaults"
    ],
    "browser": {
        "[module-name]": false
    },
    "scripts": {
        "ci": "npm ci",
        "start": "vite serve",
        "build": "vite build --emptyOutDir",
        "preview": "vite preview",
        "template": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./template-generator.ts",
        "tye": "tye run --watch --dashboard & npm start",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "test:ui": "vitest --ui --api 3333 --coverage",
        "test:coverage": "vitest run --coverage",
        "lint": "eslint --ext js,jsx,ts,tsx .",
        "lint:fix": "npm run lint:fix:eslint",
        "lint:fix:eslint": "eslint --fix --ext js,jsx,ts,tsx .",
        "clean": "rimraf dist && rimraf coverage",
        "play": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test",
        "play:ui": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --ui",
        "play:debug": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --debug",
        "play:generate": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright codegen",
        "api:generate": "cd api && npx ts-node ./generator.ts"
    },
    "dependencies": {
        "@emotion/cache": "^11.11.0",
        "@emotion/css": "^11.11.2",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@harrybin/react-common": "github:XpiritBV/react-common",
        "@mui/icons-material": "^5.14.19",
        "@mui/material": "^5.14.20",
        "@mui/utils": "^5.14.20",
        "file-saver": "^2.0.5",
        "notistack": "3.0.1",
        "process": "^0.11.10",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-intl": "6.5.5",
        "react-router-dom": "6.20.1",
        "stacktrace-js": "^2.0.2",
        "tss-react": "^4.9.3"
    },
    "devDependencies": {
        "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
        "@openapitools/openapi-generator-cli": "^2.7.0",
        "@playwright/test": "^1.40.1",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@types/fs-extra": "11.0.4",
        "@types/glob": "^8.1.0",
        "@types/node": "20.10.3",
        "@types/prompts": "^2.4.9",
        "@types/react": "18.2.42",
        "@types/react-dom": "18.2.17",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.2",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitest/coverage-v8": "^1.0.1",
        "@vitest/ui": "^1.0.1",
        "ajv": "^8.12.0",
        "browserslist": "4.22.2",
        "css-loader": "6.8.1",
        "editorconfig": "2.0.0",
        "eslint": "8.55.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.0.1",
        "eslint-plugin-react": "7.33.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "glob": "^10.3.10",
        "jsdom": "^23.0.1",
        "node-fetch": "^3.3.2",
        "prettier": "3.1.0",
        "prompts": "^2.4.2",
        "rimraf": "5.0.5",
        "style-loader": "3.3.3",
        "testcontainers": "10.3.2",
        "ts-node": "10.9.1",
        "typescript": "5.3.2",
        "unplugin-auto-import": "^0.17.2",
        "vite": "^5.0.5",
        "vite-plugin-node-polyfills": "^0.17.0",
        "vite-tsconfig-paths": "^4.2.1",
        "vitest": "^1.0.1",
        "vitest-fetch-mock": "^0.2.2",
        "vitest-sonar-reporter": "^1.0.0"
    },
    "overrides": {
        "cheerio": "1.0.0-rc.12"
    }
}