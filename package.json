{
  "name": "react-template",
  "version": "2.0.0",
  "description": "React template",
  "author": "Harald Binkle",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "browserslist": [
    "defaults"
  ],
  "browser": {
    "[module-name]": false
  },
  "scripts": {
    "ci": "npm ci",
    "start": "vite serve",
    "build": "vite build --emptyOutDir",
    "ts": "tsc",
    "preview": "vite preview",
    "tye": "tye run --watch --dashboard & npm start",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui --api 3333 --coverage",
    "test:coverage": "vitest run --coverage",
    "test:preview": "vitest-preview",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:fix": "npm run lint:fix:eslint",
    "lint:fix:eslint": "eslint --fix --ext js,jsx,ts,tsx .",
    "clean": "rimraf dist && rimraf coverage",
    "play": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test",
    "play:simple": "npx playwright test",
    "play:install": "npx playwright install || echo 'Browser install failed, continuing with system browser'",
    "play:ui": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --ui",
    "play:debug": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --debug",
    "play:generate": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright codegen"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@harrybin/react-common": "^0.0.17",
    "@jridgewell/sourcemap-codec": "^1.5.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@mui/utils": "^7.1.0",
    "notistack": "3.0.2",
    "process": "^0.11.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intl": "7.1.11",
    "react-router-dom": "^7.5.3",
    "stacktracey": "^2.1.8",
    "tss-react": "^4.9.18",
    "vite-plugin-svgr": "^4.3.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@playwright/test": "^1.52.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "22.15.16",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@typescript-eslint/eslint-plugin": "8.32.0",
    "@typescript-eslint/parser": "8.32.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "browserslist": "4.24.5",
    "editorconfig": "2.0.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "10.1.3",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "style-loader": "4.0.0",
    "testcontainers": "10.25.0",
    "typescript": "5.8.3",
    "unplugin-auto-import": "^19.2.0",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-node-polyfills": "^0.23.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3",
    "vitest-fetch-mock": "^0.4.5",
    "vitest-preview": "^0.0.2",
    "vitest-sonar-reporter": "^2.0.1"
  }
}
