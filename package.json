{
  "name": "react",
  "version": "1.0.0",
  "description": "React template",
  "author": "Harald Binkle",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.12.0"
  },
  "browserslist": [
    "defaults"
  ],
  "browser": {
    "[module-name]": false
  },
  "scripts": {
    "ci": "npm ci",
    "start": "vite serve",
    "build": "vite build",
    "preview": "vite preview",
    "tye": "tye run --watch --dashboard & npm start",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui --api 3333 --coverage",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:fix": "npm run lint:fix:eslint",
    "lint:fix:eslint": "eslint --fix --ext js,jsx,ts,tsx .",
    "clean": "rimraf dist && rimraf coverage",
    "play": "../net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test",
    "play:ui": "../net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --ui",
    "play:debug": "../net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --debug",
    "play:generate": "../net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright codegen",
    "api:generate": "cd api && npx ts-node ./generator.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
