{
  "name": "react-template",
  "version": "1.0.0",
  "description": "React template",
  "author": "Harald Binkle",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20.5.0"
  },
  "browserslist": [
    "defaults"
  ],
  "browser": {
    "[module-name]": false
  },
  "scripts": {
    "ci": "npm ci",
    "start": "vite serve",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "tye": "tye run --watch --dashboard & npm start",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui --api 3333 --coverage",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "lint:fix": "npm run lint:fix:eslint",
    "lint:fix:eslint": "eslint --fix --ext js,jsx,ts,tsx .",
    "clean": "rimraf dist && rimraf coverage",
    "play": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test",
    "play:ui": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --ui",
    "play:debug": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright test --debug",
    "play:generate": "../template-net7srv/DotnetApiTemplate/buildDockerImage.sh && npx playwright codegen",
    "api:generate": "cd api && npx ts-node ./generator.ts"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@harrybin/react-common": "^0.0.11",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/utils": "^5.15.9",
    "notistack": "3.0.1",
    "process": "^0.11.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "6.6.2",
    "react-router-dom": "6.22.0",
    "stacktrace-js": "^2.0.2",
    "tss-react": "^4.9.4"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "@playwright/test": "^1.41.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "20.11.19",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "browserslist": "4.23.0",
    "css-loader": "6.10.0",
    "editorconfig": "2.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "^24.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "style-loader": "3.3.4",
    "testcontainers": "10.7.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.1.3",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2",
    "vitest-fetch-mock": "^0.2.2",
    "vitest-sonar-reporter": "^1.0.0"
  }
}
